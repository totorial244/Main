local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local gui = Instance.new("ScreenGui", LocalPlayer:WaitForChild("PlayerGui"))
local frame = Instance.new("Frame", gui)
frame.Size = UDim2.new(0, 200, 0, 260)
frame.Position = UDim2.new(0, 10, 0, 100)
frame.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
frame.Active = true
frame.Draggable = true

local function createButton(text, posY, callback)
	local btn = Instance.new("TextButton", frame)
	btn.Size = UDim2.new(1, -20, 0, 30)
	btn.Position = UDim2.new(0, 10, 0, posY)
	btn.Text = text
	btn.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
	btn.TextColor3 = Color3.new(1,1,1)
	btn.MouseButton1Click:Connect(callback)
end

local function promptName()
	return game:GetService("Players").LocalPlayer.Name
end

local function getPlayer(name)
	for _, plr in pairs(Players:GetPlayers()) do
		if plr.Name:lower():sub(1, #name) == name:lower() then
			return plr
		end
	end
end

-- Bot√µes

createButton("Kick Player", 5, function()
	local name = game:GetService("StarterGui"):Prompt("Nome do player:")
	local plr = getPlayer(name)
	if plr then plr:Kick("Kicado pelo painel admin.") end
end)

createButton("Jail Player", 40, function()
	local name = game:GetService("StarterGui"):Prompt("Nome do player:")
	local plr = getPlayer(name)
	if plr and plr.Character then
		plr.Character:MoveTo(Vector3.new(-1000, 100, -1000))
	end
end)

createButton("Kill Player", 75, function()
	local name = game:GetService("StarterGui"):Prompt("Nome do player:")
	local plr = getPlayer(name)
	if plr and plr.Character and plr.Character:FindFirstChild("Humanoid") then
		plr.Character.Humanoid.Health = 0
	end
end)

createButton("Fling Player", 110, function()
	local name = game:GetService("StarterGui"):Prompt("Nome do player:")
	local plr = getPlayer(name)
	if plr and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
		plr.Character.HumanoidRootPart.Velocity = Vector3.new(9999, 9999, 9999)
	end
end)

createButton("Speed +10", 145, function()
	local char = LocalPlayer.Character
	if char and char:FindFirstChild("Humanoid") then
		char.Humanoid.WalkSpeed += 10
	end
end)

createButton("Speed -10", 180, function()
	local char = LocalPlayer.Character
	if char and char:FindFirstChild("Humanoid") then
		char.Humanoid.WalkSpeed -= 10
	end
end)

createButton("Noclip", 215, function()
	local char = LocalPlayer.Character
	if char then
		for _, part in pairs(char:GetChildren()) do
			if part:IsA("BasePart") then
				part.CanCollide = false
			end
		end
	end
end)
